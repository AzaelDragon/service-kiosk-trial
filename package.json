{
  "name": "service-kiosk-trial",
  "version": "0.0.1",
  "description": "A simple service order ticketing API powered by NestJS and GraphQL. Built as a coding trial for Imaginamos S.A.S.",
  "main": "dist/main.js",
  "repository": "git@github.com:AzaelDragon/service-kiosk-trial.git",
  "author": "David S. García (Azæl) <azael@dragonspark.tech>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "docker": "docker-compose up -d",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:save": "yarn run prisma migrate save --experimental",
    "prisma:up": "yarn run prisma migrate up --experimental",
    "prisma:studio": "yarn run prisma studio --experimental",
    "prisma:generate": "yarn run prisma generate",
    "prisma:generate:watch": "yarn run prisma generate --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose -p service-kiosk up",
    "docker:up:bg": "docker-compose -p service-kiosk up -d",
    "docker:up:build": "docker-compose up --build -V"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.2",
    "@nestjs/graphql": "^7.6.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.2",
    "@nestjs/swagger": "^4.5.12",
    "@prisma/client": "^2.3.0",
    "apollo-server-express": "^2.16.1",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.0.15",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@apollo/gateway": "^0.19.0",
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.4.2",
    "@prisma/cli": "^2.3.0",
    "@types/bcrypt": "^3.0.0",
    "@types/chance": "^1.1.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "chance": "^1.1.6",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7"
  }
}
